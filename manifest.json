{
  "name": "Unstar",
    "version": "1.0",
    "background": {"scripts": ["background.js"]},
    "manifest_version": 2,
    "description": "This provides a toggle to hide/show any yelp star reviews.",
    "browser_action": {
      "default_icon": "inactive_icon.png"
    },
    "permissions": [
         "tabs", "http://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["http://*.yelp.com/*"],
        "run_at": "document_end",
        "js": ["jquery.js"]
      }
    ]
}
